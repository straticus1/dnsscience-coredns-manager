{
  "name": "n8n-nodes-dnsscience",
  "version": "0.1.0",
  "description": "n8n nodes for DNS Science Toolkit - CoreDNS and Unbound management",
  "keywords": [
    "n8n-community-node-package",
    "dns",
    "coredns",
    "unbound",
    "devops"
  ],
  "license": "Apache-2.0",
  "homepage": "https://dnsscience.io",
  "author": {
    "name": "After Dark Systems, LLC",
    "email": "hello@dnsscience.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dnsscience/n8n-nodes-dnsscience.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint nodes credentials --ext .ts",
    "lintfix": "eslint nodes credentials --ext .ts --fix"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DnsScienceApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/DnsScience/DnsScience.node.js",
      "dist/nodes/DnsQuery/DnsQuery.node.js",
      "dist/nodes/DnsMigrate/DnsMigrate.node.js",
      "dist/nodes/DnsCompare/DnsCompare.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "typescript": "^5.0.0"
  }
}
